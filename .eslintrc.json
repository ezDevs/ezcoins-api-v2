{
  "env": {
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "implicit-arrow-linebreak": "off",
    "arrow-parens": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "camelcase": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "max-len": "off"
  }
}
